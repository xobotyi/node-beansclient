{
  "name": "node-beanstalk",
  "description": "The most comprehensive beanstalk client for nodejs",
  "keywords": [
    "nodejs",
    "beanstalkd",
    "beanstalk",
    "node-beanstalk"
  ],
  "version": "1.6.7",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xobotyi/node-beanstalk.git"
  },
  "bugs": {
    "url": "https://github.com/react-hookz/web/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.8",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.1",
    "@types/jest": "^27.4.0",
    "@xobotyi/eslint-config": "^2.3.0",
    "commitlint": "^16.0.2",
    "cpy-cli": "^3.1.1",
    "eslint": "^8.6.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.7",
    "rimraf": "^3.0.2",
    "semantic-release": "^18.0.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typedoc-neo-theme": "^1.1.1",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "docs:buid": "yarn typedoc && cpy .static/typedoc/style.css docs/assets/css/",
    "build": "yarn build:cleanup && yarn build:tsc",
    "build:cleanup": "rimraf ./dist",
    "build:tsc": "tsc -p ./tsconfig.build.json",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint . --ext ts,js,tsx,jsx",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.[tj]s?x": "eslint --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
