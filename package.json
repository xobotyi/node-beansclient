{
  "name": "node-beanstalk",
  "description": "The most comprehensive beanstalk client for nodejs",
  "keywords": [
    "nodejs",
    "beanstalkd",
    "beanstalk",
    "node-beanstalk"
  ],
  "version": "1.6.8",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xobotyi/node-beanstalk.git"
  },
  "bugs": {
    "url": "https://github.com/react-hookz/web/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@jamesacarr/jest-reporter-github-actions": "^0.0.4",
    "@react-hookz/eslint-formatter-gha": "^2.0.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.3",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.7",
    "@xobotyi/eslint-config": "^2.3.0",
    "commitlint": "^19.2.2",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "rimraf": "^5.0.5",
    "semantic-release": "^23.0.8",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "docs:build": "yarn typedoc && cpy .static/typedoc/style.css docs/assets/css/",
    "build": "yarn build:cleanup && yarn build:tsc",
    "build:cleanup": "rimraf ./dist",
    "build:tsc": "tsc -p ./tsconfig.build.json",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint . --ext ts,js,tsx,jsx",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.[tj]s?x": "eslint --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "packageManager": "yarn@4.1.0"
}
